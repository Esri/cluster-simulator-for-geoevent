                  <Button cancelButton="true" mnemonicParsing="false" onAction="#onCancelButtonPushed" text="Cancel" />
                  <Button fx:id="okButton" defaultButton="true" mnemonicParsing="false" onAction="#onOKButtonPushed" text="OK" />
               </children>
               <padding>
                  <Insets right="10.0" />
               </padding>
               <VBox.margin>
